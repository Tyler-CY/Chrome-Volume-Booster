{
  "name": "Chrome Volume Booster",
  "description": "For boosting the volume of YouTube videos.",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "icons/moviemusicicon16.png",
    "64": "icons/moviemusicicon64.png",
    "128": "icons/moviemusicicon128.png"
  }
}